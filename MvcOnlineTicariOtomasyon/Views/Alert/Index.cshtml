
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<script src="~/Scripts/jquery-3.7.0.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<br />
<br />
<button class="btn btn-primary" id="btn1">buton1</button>
<button class="btn btn-warning" id="btn2">buton2</button>
<button class="btn btn-info" id="btn3">buton3</button>
<button class="btn btn-dark" id="btn4">buton4</button>
<button class="btn btn-danger" id="btn5">buton5</button>
<button class="btn btn-danger" id="btn6">buton6</button>

<script>
    $('#btn1').click(function () {
        swal("Hello world!");
    });
</script>

<script>
    $('#btn2').click(function () {
        swal("Here's the title!", "...and here's the text!");
    });
</script>

<script>
    $('#btn3').click(function () {
        swal("Good job!", "You clicked the button!", "success");
    });
</script>

<script>
    $('#btn4').click(function () {
        swal({
            title: "Good job!",
            text: "You clicked the button!",
            icon: "success",
        });
    });
</script>

<script>
    $('#btn5').click(function () {
        swal({
            title: "Good job!",
            text: "You clicked the button!",
            icon: "success",
            button: "Aww yiss!",
        });
    });
</script>

<script>
    $('#btn6').click(function () {
        swal("Good job!", "You clicked the button!", "success", {
            button: "Aww yiss!",
        });
    });
</script>



